#define(String markdownFile, String url)
#set(String htmlContents = markdown(markdownFile))

#if (!url.startsWith("faq") && !url.startsWith("about") && !url.startsWith("history"))
    <style type="text/css">
        .affix-top {
          float: left;
          margin-top: 20px;
        }
        .affix {
          position: left;
          float: right;
          top: 20px;
          margin-top: 0 !important;
          z-index: 100;
        }
        .jb-nav-list-container {
            width: 220px !important;
            margin-top: 20px;
        }
        .markdown {
          margin-left: 260px; 
        }
    </style>
    <div class="jb-nav-list-container" data-spy="affix" data-offset-top="230">
        <ul class="jb-nav-list">
        #for(Map.Entry<String, String> entry : getHeaders(htmlContents, url == "syntax.html" ? 1: 2))
            <li><a href="${entry.key}">${entry.value}</a></li>
        #end
        </ul>
    </div>
#end

    <div class="markdown">

<!-- markdown body begin -->
#if (url.startsWith("faq-"))
	${markdown("faq.md")}
#end

    ${htmlContents}

#if (url == "syntax.html")
    ${read("velocity.html")}
#end
<!-- markdown body end -->

